<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wzc.jetpackdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JetPackDemo">
        <activity android:name=".paging2.DbAndNetworkActivity"></activity>
        <activity android:name=".paging2.PositionActivity" />
        <activity android:name=".paging2.PageKeyActivity" />
        <activity android:name=".paging2.ItemKeyActivity" />
        <activity android:name=".paging3.Paging3Activity" />
        <activity android:name=".paging2.Paging2Activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>